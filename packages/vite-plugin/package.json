{
  "packageManager": "yarn@4.4.0",
  "name": "@viteyaml/plugin",
  "version": "0.1.0",
  "type": "module",
  "types": "./dist/viteyaml.plugin.d.ts",
  "main": "./dist/viteyaml.plugin.js",
  "module": "./dist/viteyaml.plugin.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/viteyaml.plugin.js",
      "require": "./dist/viteyaml.plugin.cjs",
      "types": "./dist/viteyaml.plugin.d.ts"
    },
    "./yaml": {
      "types": "./dist/viteyaml.yaml.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint -c ../../shared/eslint.config.js",
    "format": "prettier --config ../../shared/prettier.config 'src/**/*.ts' --write",
    "test": "jest --collect-coverage",
    "deploy": "npm publish --access=public",
    "build": "yarn tsup --config ./build.config.ts"
  },
  "devDependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "@types/eslint": "^9.6.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "@types/yaml": "^1.9.7",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-html-reporter": "^3.10.2",
    "jest-junit": "^16.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.0"
  },
  "peerDependencies": {
    "@rollup/pluginutils": "^5.1.0",
    "vite": "^5.3.5"
  },
  "dependencies": {
    "@viteyaml/yaml-types": "workspace:^",
    "yaml": "^2.5.0"
  }
}
